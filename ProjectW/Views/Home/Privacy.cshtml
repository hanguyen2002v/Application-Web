@{
    ViewData["Title"] = "Privacy Policy";
}
@model List<ProjectW.Models.Book>
<h1>@ViewData["Title"]</h1>

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4 mb-3">
                <div class="card" style="width: 18rem;">
                    <img src="@item.Image" class="card-img-top" alt="..." width="50" height="250">
                    <div class="card-body">
                        <h5 class="card-title">
                            Title: @item.Image
                        </h5>
                        <p class="card-text">
                            Description: @item.Image
                        </p>
                        <p class="card-text">
                            Price: @item.Image$
                        </p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>



